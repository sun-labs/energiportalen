    "test": "tests"
  },
  "dependencies": {},
  "devDependencies": {
    "chai": "^3.5.0",
    "concurrently": "^3.4.0"
  },
  "scripts": {
    "test_be": "cd tests/; webpack && mocha build/backend.js",
    "test_fe": "cd tests/; webpack && mocha build/frontend.js",
